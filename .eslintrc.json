{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": 0,
        "no-extra-boolean-cast": 0,
        "no-unsafe-optional-chaining": 0
    }
}
